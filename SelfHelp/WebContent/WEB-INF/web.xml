<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>com.jenish.shg.RegisterServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/reg</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>display</servlet-name>
		<servlet-class>com.jenish.shg.DisplayServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>display</servlet-name>
		<url-pattern>/disp</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.jenish.shg.LoginServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>finishing</servlet-name>
		<servlet-class>com.jenish.shg.FinalUpdate</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>finishing</servlet-name>
		<url-pattern>/finish</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>changepage</servlet-name>
		<servlet-class>com.jenish.shg.ChangePage</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>changepage</servlet-name>
		<url-pattern>/change</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>updateProfile</servlet-name>
		<servlet-class>com.jenish.shg.UpDelServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>updateProfile</servlet-name>
		<url-pattern>/updateDelete</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>logingout</servlet-name>
		<servlet-class>com.jenish.shg.LogoutServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>logingout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>